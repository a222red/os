{
    "configurations": [
        {
            "name": "OsDev",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-clang-x86",
            "compilerArgs": [
                "-m32",
                "-std=c++20",
                "-nostdlib",
                "-nostartfiles",
                "-fno-builtin",
                "-nodefaultlibs",
                "-static",
                "-fno-pic",
                "-fno-pie",
                "-ffreestanding",
                "-fno-stack-protector",
                "-fno-asynchronous-unwind-tables"
            ]
        }
    ],
    "version": 4
}